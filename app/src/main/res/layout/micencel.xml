 <AbsoluteLayout
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     xmlns:android="http://schemas.android.com/apk/res/android"
     android:id="@+id/miCenceles"
     android:background="@drawable/fond1">

     <LinearLayout
         android:id="@+id/textos"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:padding="23dp"
         android:orientation="vertical"
         android:layout_x="2dp"
         android:layout_y="0dp"
         android:weightSum="1"
         android:background="@null"
         android:gravity="center">

         <ImageView
             android:layout_width="61dp"
             android:layout_height="95dp"
             android:id="@+id/icocen"
             android:layout_x="118dp"
             android:layout_y="111dp"
             android:background="@drawable/iconocen"
             android:layout_gravity="center_horizontal" />

         <TextView
             android:id="@+id/regisusu"
             android:layout_width="match_parent"
             android:layout_height="-94dp"
             android:text="Iniciar Sesión"
             android:textSize="17dp"
             android:paddingTop="5dp"
             android:paddingLeft="10dp"
             android:textColor="#ffffffff"
             android:layout_above="@+id/areacomp"
             android:layout_centerHorizontal="true"
             android:gravity="center"
             android:paddingBottom="19dp" />

         <EditText
             android:id="@+id/txtusuario"
             android:layout_width="match_parent"
             android:layout_height="38dp"
             android:textSize="15dp"
             android:hint="Usuario"
             android:background="@drawable/fnini"
             android:paddingLeft="3dp"
             android:textColor="#ff0d0d0d"
             android:textColorHint="#ff0d0d0d"
             android:singleLine="true"
             android:gravity="center" />
         <TextView
             android:id="@+id/invi"
             android:layout_width="205dp"
             android:layout_height="wrap_content"
             android:textSize="15dp"
             android:hint="invi"
             android:layout_weight="0.25"
             android:visibility="invisible" />
         <EditText
             android:id="@+id/txtcontra"
             android:layout_width="match_parent"
             android:layout_height="38dp"
             android:hint="Contraseña"
             android:textSize="15dp"
             android:background="@drawable/fnini"
             android:paddingLeft="3dp"
             android:textColorHint="#ff0d0d0d"
             android:singleLine="true"
             android:textColor="#ff0d0d0d"
             android:gravity="center" />




         <TextView
             android:id="@+id/invi2"
             android:layout_width="205dp"
             android:layout_height="wrap_content"
             android:textSize="15dp"
             android:hint="invi"
             android:layout_weight="0.25"
             android:visibility="invisible" />
         <Button
             android:id="@+id/btoingresa"
             android:layout_width="match_parent"
             android:layout_height="38dp"
             android:background="@drawable/fnbtoin"
             android:text="Ingresa"
             android:layout_x="-1dp"
             android:layout_y="461dp"
             android:textColor="#ffffffff"
             android:textColorHint="#ffffffff" />



         <Button
             android:layout_width="fill_parent"
             android:layout_height="wrap_content"
             android:text="Inicia Sesion con Facebook"
             android:id="@+id/facebook"
             android:layout_gravity="center_horizontal"
             android:background="#3b5998"
             android:layout_margin="8dp"
             android:textColor="#ffffffff"
             android:onClick="RegistroFacebook" />




         <TextView
             android:id="@+id/registra"
             android:layout_width="match_parent"
             android:layout_height="21dp"
             android:text="¿No estas registrado?, Registrate Aquí"
             android:linksClickable="true"
             android:longClickable="true"
             android:autoLink="all"
             android:clickable="true"
             android:enabled="true"
             android:onClick="Registro"
             android:layout_x="3dp"
             android:layout_y="494dp"
             android:textColor="#ffffffff"
             android:gravity="center" />
         <TextView
             android:id="@+id/contolvi"
             android:layout_width="match_parent"
             android:layout_height="21dp"
             android:text="¿Olvidaste tu contraseña? Recuperala aquí"
             android:linksClickable="true"
             android:longClickable="true"
             android:autoLink="all"
             android:clickable="true"
             android:enabled="true"
             android:layout_x="3dp"
             android:layout_y="494dp"
             android:gravity="center|center_vertical"
             android:textColor="#ffffffff"
             android:onClick="CambiaContra" />


     </LinearLayout>

    <!-- </AbsoluteLayout> -->


    </AbsoluteLayout>